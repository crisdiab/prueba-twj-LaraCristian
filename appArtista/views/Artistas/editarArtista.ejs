<div style='background-color:white;color:black;'>

  <h1>Edita el Artista  </h1>

  <div class="row">
    <div class="col-sm-8">
      <form action="Artista/editarArtista" method="post">
        <div class="form-group">
          <label for="exampleInputEmail1">Nombres</label>
          <input minlength=5 required type="text" class="form-control"  placeholder="Nombres" name="nombre">
        </div>

        <div class="form-group">
          <label for="exampleInputEmail1">Estilo</label>
          <select name="estilo" id="">
            <option value="Pop">Pop</option>
            <option value="Rock">Rock</option>
          </select>

        </div>
        <div class="form-group">
          <label for="exampleInputEmail1">Pais de Residencia</label>
          <input type="text" class="form-control"  placeholder="Pais Residencia" name="paisResidencia">
        </div>
        <input type="hidden" name="id" value="<%=artista.id%>">
        <button type="submit" class="btn btn-success">Editar Usuario</button>
        <br>

      </form>
    </div>
    <div class="col-sm-4">



    </div>
  </div>


</div>
